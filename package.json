{
  "name": "salvation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "force:build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "outdated": "yarn outdated",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "sync": "rimraf --glob ../riddance/app/src/main/assets/* && syncdir dist/ ../riddance/app/src/main/assets --exclude content -do --quiet"
  },
  "dependencies": {
    "@react-three/drei": "^9.89.0",
    "@react-three/fiber": "^8.15.11",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-particles" : "^2.12.2",
    "react-router-dom": "^6.20.0",
    "usehooks-ts": "^2.9.1",
    "tailwind-merge": "^2.0.0",
    "three": "^0.159.0",
    "tsparticles": "^2.12.0",
    "tsparticles-preset-confetti": "^2.12.0",
    "tsparticles-preset-snow": "^2.12.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@heroicons/react": "^2.0.18",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/three": "^0.159.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sync-directory": "^6.0.5",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
